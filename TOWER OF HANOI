#include <stdio.h>
void TOH(int n,char s,char d,char t)
{
    if(n>1){
        TOH((n-1),s,t,d);
        printf("\n move %d list from %C to %C",n,s,d);
        TOH(n-1,t,d,s);
    }
    else
        printf("\n move %d disc from %C to %C",n,s,d);
}
int main()
{
    int n;
    printf("\n Read number of discs:");
    scanf("%d",&n);
    TOH(n,'s','D','T');
    return 0;
}
